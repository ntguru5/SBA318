<!DOCTYPE html>
  <html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pets</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <h1>Pets</h1>
  <ul>
    <% pets.forEach((pet) => { %>
      <li>
        <a href="/pets/<%= pet.id %>"><%= pet.name %></a>
        <img src="/uploads/<%= pet.profilePicture %>" alt="Profile Picture">
      </li>
    <% }); %>
  </ul>
  <form action="/pets" method="post" enctype="multipart/form-data">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name"><br><br>
    <label for="breed">Breed:</label>
    <input type="text" id="breed" name="breed"><br><br>
    <label for="sex">Sex:</label>
    <input type="text" id="sex" name="sex"><br><br>
    <label for="age">Age:</label>
    <input type="number" id="age" name="age"><br><br>
    <label for="profilePicture">Profile Picture:</label>
    <input type="file" id="profilePicture" name="profilePicture"><br><br>
    <input type="submit" value="Add Pet">
  </form>
</body>
  </body>
</html>
